<?xml version="1.0" encoding="ISO-8859-1"?>
<presentation status="incomplete">
  <title>Example Presentation</title>

  <slide>
    <title>First Slide</title>

    <visual fileref="images/demo.svg"></visual>

    <notes>
      <student>
        <para>Student notes go here.</para>

        <para>If there are no student notes, use the presenter notes for
        students.</para>

        <para>and vice versa if there are no presenter notes.</para>
      </student>

      <presenter>
        <para>Presenter notes go here.</para>
      </presenter>
    </notes>
  </slide>

  <slide>
    <title>Second Slide</title>

    <visual fileref="../shared/deep-sea-divers.jpg"></visual>

    <notes>
      <student></student>

      <presenter></presenter>
    </notes>
  </slide>
</presentation>
